<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="framgiavn.project02.web.model.CourseSubject" table="Course_Subject"
		dynamic-insert="true" dynamic-update="true">
		<id name="id" type="java.lang.Integer">
			<column name="id" length="10" />
			<generator class="increment" />
		</id>
		
		<property name="courseId" type="java.lang.Integer">
			<column name="courseId" length="10" />
		</property>	
		
		<property name="subjectId" type="java.lang.Integer">
			<column name="subjectId" length="10" />
		</property>

		<property name="status" type="java.lang.String">
			<column name="status" length="45" />
		</property>
	</class>
	
</hibernate-mapping>